  "version": "0.0.1",
  "description": "Postcss plugin for add wrapper to your styles",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "repository": {
    "type": "git",
